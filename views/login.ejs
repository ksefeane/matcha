<%- include ('partials/header') %>
<h2 class="text-warning">login</h2><br>
<form action="/login" method="post">
	<div class="form-group col-md-4">
		<label for="username" class="text-warning">username</label>
		<input type="text" id="username" name="username" class="form-control">
	</div>
	<div class="form-group col-md-4">
		<label for="password" class="text-warning">password</label>
		<input type="password" id="password" name="password" class="form-control">
	</div>
	<% if (e) {%>
		<p style="color:red"><%=e%></p>
	<%}%>
	<div class="form-group col-md-4">
		<input type="submit" value="login" class="btn bg-danger">
	</div>
</form><br>
<a href="./f" class="text-warning">forgot password?</a>
<%- include ('partials/footer') %>
