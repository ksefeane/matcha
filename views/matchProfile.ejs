<%- include ('partials/header') %>
<img src='/uploads/<%=match.username%>' width="200" height="200">
<% if (adminToken) { %>
    <button class="block" id="<%=match.username%>">suspend</button>
<%} %>
<button class="like" id="<%=match.username%>">like</button>
<p class="list-group-item"><%= match.username %></p>
<p class="list-group-item"><%= match.age %></p>
<p class="list-group-item"><%= match.gender %></p>
<p class="list-group-item"><%= match.orientation %></p>
<p class="list-group-item"><%= match.preference %></p>
<p class="list-group-item"><%= match.interests %></p>
<p class="list-group-item"><%= match.location %></p>
<p class="list-group-item"><%= match.bio %></p>
<script src="/js/like.js"></script>
<script src="/js/block.js"></script>
<%- include ('partials/footer') %>
